{
    "name": "Quantum",
    "version": "0.22",
    "input": {
        "format": "xyz",
        "parameters": {
            "basis": {
                "type": "string",
                "description": "The basis set.",
                "default": "sto3g"
            },
            "charge": {
                "type": "number",
                "description": "The net charge of the system.",
                "default": 0
            },
            "multiplicity": {
                "type": "number",
                "description": "The spin multiplicity.",
                "default": 1
            },
            "orbital_reduction": {
                "type": "array",
                "description": "Complexity of the calculation",
                "default": [4, 10]
            }
        }
    },
    "output": {
        "format": "cjson"
    }
}